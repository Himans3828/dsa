#include <iostream>
using namespace std;
int main() {
int n;
  cout << "entet the amount" << "   " << endl;
  cin>> n;
  int Rs100 , Rs50 , Rs20 , Rs10;
  switch (1) {

    case 1: Rs100 = n/100;
             n = n % 100;
             cout << "no. of 100 ruppes notes required are" << Rs100 << endl ;

    case 2: Rs50 = n/50;
             n = n % 50;
             cout << "no. of 50 ruppes notes required are" << Rs50 << endl ;


    case 3: Rs20 = n/20;
             n = n % 20;
             cout << "no. of 20 ruppes notes requires are" << Rs20 << endl ;
    

    case 4: Rs10 = n/10;
             n = n % 10;
             cout << "no. of 10 ruppes notes required are" << Rs10 << endl ;
    
   }



  
}
    
